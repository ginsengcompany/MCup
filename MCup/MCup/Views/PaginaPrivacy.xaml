<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MCup.Views.PaginaPrivacy">
    <ContentPage.Content>
        <StackLayout>
            <Button 
                Command="{Binding infoPrivacy}"  
                Text="infoPrivacy"
                />
            <Button 
                Command="{Binding datiUtente}"  
                Text="datiUtente"
                />
            <Button 
                Command="{Binding eliminaUtente}"  
                Text="elimina Account principale"
                />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>